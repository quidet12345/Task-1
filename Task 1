favfruitsnum = int(input("Enter num of fruits : "))
nameoffruits = []

for i in range(favfruitsnum):
    fruitname = input("Enter name of fruits : ")
    nameoffruits.append(fruitname)

    if fruitname == "apple":
        print("Happy Eating")

    elif fruitname == "banana":
        break

print(nameoffruits)